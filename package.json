    "@0xpass/passport-viem": "^2.0.1",
    "@alchemy/aa-alchemy": "^3.18.2",
    "@alchemy/aa-core": "^3.18.2",
    "@tanstack/react-query": "^5.49.2",
    "@wagmi/core": "^2.11.6",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
